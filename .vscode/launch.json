{ //launch.json
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        
    {
        "name": "Launch Chrome",
        "request": "launch",
        "type": "pwa-chrome",
        "url": "http://localhost:8080",
        
        "webRoot": "${workspaceFolder}"
    },
    {
        "type": "robotframework-lsp",
        "name": "Robot Framework: Launch .robot file",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "target": "${file}",
        "terminal": "none",
        "env": {},
        "args": []
    }
    ],
    "compounds": [],
    "type": "robot",
    "request": "launch",
    "name": "Robotframework Debugger",
    "program": "${workspaceFolder}/${relativeFile}",
    "stopOnEntry": true,
    "arguments": "",
    "libraries": 
    [
    "${workspaceFolder}\\Libraries"
    ],
    "variables": 
        [
            {
                "Name": "RobotVariable",
                "Value": "data"
            },
        ],
}